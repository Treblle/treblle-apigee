<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ServiceCallout continueOnError="false" enabled="true" name="SC-callout">
  <DisplayName>SC-callout</DisplayName>
  <Properties/>
  <Request clearPayload="true">
    <Set>
      <Headers>
        <Header name="Content-Type">{treblle_content_type}</Header>
        <Header name="x-api-key">{treblle_x_api_key}</Header>
        <Header name="User-Agent">{treblle_user_agent}</Header>
      </Headers>
      <Payload>{treblle_payload}</Payload>
      <Verb>POST</Verb>
    </Set>
  </Request>
  <Response>treblle_response</Response>
  <HTTPTargetConnection>
    <Properties/>
    <URL>https://rocknrolla.treblle.com</URL>
  </HTTPTargetConnection>
</ServiceCallout>
