<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations mapIdentifier="{YOUR_KVM_NAME}" continueOnError="false" enabled="true"
    name="KVM-GetTreblleCredentials">
    <DisplayName>Get Treblle Credentials</DisplayName>
    <Properties />
    <ExclusiveCache>false</ExclusiveCache>
    <ExpiryTimeInSecs>300</ExpiryTimeInSecs>
    <Get assignTo="treblle.sdk.token">
        <Key>
            <Parameter>treblle_sdk_token</Parameter>
        </Key>
    </Get>
    <Get assignTo="treblle.api.key">
        <Key>
            <Parameter>treblle_api_key</Parameter>
        </Key>
    </Get>
    <Scope>environment</Scope>
</KeyValueMapOperations>